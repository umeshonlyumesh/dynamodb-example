SELECT 
    CASE
        WHEN UPPER(cps_state) = 'NEW'      AND UPPER(status) = 'REPAIR'            THEN 'Processing'
        WHEN UPPER(cps_state) = 'ACTIVE'   AND UPPER(status) = 'REPAIR'            THEN 'Processing'
        WHEN UPPER(cps_state) = 'COMPLETED'AND UPPER(status) = 'COMPLETE'          THEN 'Completed'
        WHEN UPPER(cps_state) = 'CANCELLED'AND UPPER(status) = 'CANCELLED'         THEN 'Canceled'
        WHEN UPPER(cps_state) = 'REJECTED' AND UPPER(status) = 'REJECTED'          THEN 'Rejected'
        WHEN UPPER(cps_state) = 'POSTEX'   AND UPPER(status) = 'POSTEX'            THEN 'Processing'
        WHEN UPPER(cps_state) = 'REPAIR'   AND UPPER(status) = 'REPAIR'            THEN 'Processing'
        WHEN UPPER(cps_state) = 'HELD'     AND UPPER(status) = 'WAIT_CONFIRMATION' THEN 'Processing'
        WHEN UPPER(cps_state) = 'RETURNED' AND UPPER(status) = 'RETURNED'          THEN 'Returned'
        WHEN UPPER(cps_state) = 'NA'       AND UPPER(status) = 'NAK'               THEN 'Rejected'
        WHEN UPPER(cps_state) = 'ACTIVE'   AND UPPER(status) IN ('CDBWAIT','BIWAITQ','CDBSTOP','MP_WAIT','VERIFY','MP_STOP')
                                                                               THEN 'Processing'
        WHEN UPPER(cps_state) = 'NA'       AND UPPER(status) IN ('WAITRETURN','PEND_APPROV')
                                                                               THEN 'Processing'
        WHEN UPPER(cps_state) = 'HELD'     AND UPPER(status) = 'NSF'               THEN 'Processing'
        ELSE 'Unknown'
    END AS business_status
FROM your_table;
