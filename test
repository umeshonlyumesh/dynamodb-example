# Kafka SSL Configuration
spring.kafka.security.protocol=SSL
spring.kafka.ssl.key-store-type=PKCS12
spring.kafka.ssl.key-store-location=file:/path/to/certificate.pfx
spring.kafka.ssl.key-store-password=pfx-password
spring.kafka.ssl.key-password=pfx-password
spring.kafka.ssl.trust-store-type=PKCS12
spring.kafka.ssl.trust-store-location=file:/path/to/certificate.pfx
spring.kafka.ssl.trust-store-password=pfx-password
spring.kafka.ssl.protocol=TLSv1.2
